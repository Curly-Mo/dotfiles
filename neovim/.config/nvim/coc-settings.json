{
  // "coc.preferences.diagnostic.displayByAle": true,
  "languageserver": {
    "metals": {
      "command": "metals-vim",
      "rootPatterns": ["build.sbt"],
      "filetypes": ["scala", "sbt"]
    }
  },
  // use microsoft python langauge server
  "python.jediEnabled": false,
  "python.formatting.provider": "black",
  "python.autoComplete.extraPaths": [
    "${workspaceFolder}/voice-nyc-recommend-pipeline/src/main/python",
  ],
}
