### APPLICATIONS ###
alt + d
  launch_or_focus alacritty
alt + F2
  launch_or_focus alacritty
# alt + f
#   launch_or_focus google-chrome
alt + f
  launch_or_focus firefox
alt + s
  launch_or_focus spotify
alt + a
  launch_or_focus slack
alt + shift + s
  launch_or_focus idea
alt + shift + g
  launch_or_focus gpclient

### SELECTION ###
alt + c
  xsel -po|xsel -bi
alt + shift + c
  xsel -po|xsel -bi
alt + @v
  xdotool type --window "$(xdotool getwindowfocus)" --clearmodifiers --delay 1 -- "$(xclip -out -selection clipboard -rmlastnl)"
alt + shift + @v
  xdotool type --window "$(xdotool getwindowfocus)" --clearmodifiers --delay 1 -- "$(xclip -out -selection clipboard -rmlastnl)"
