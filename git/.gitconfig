[user]
	name = Colin Fahy
	email = cpfahy@gmail.com
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[core]
	editor = vim
	askpass = /usr/bin/ksshaskpass
[push]
	default = simple
[credential]
	helper = cache --timeout=2600000
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
